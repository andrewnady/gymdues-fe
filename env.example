# Environment Variables for GymDues Next.js Application
# Copy this file to .env and update with your actual values

# Node Environment (MUST be production for cPanel deployment)
NODE_ENV=production

# Application Port (cPanel will set this automatically, but you can override)
PORT=3000

# API Base URL - Production API endpoint
# IMPORTANT: This is the production API URL for cPanel deployment
NEXT_PUBLIC_API_BASE_URL=https://cms.gymdues.com

# Hostname (usually localhost for cPanel)
HOSTNAME=localhost
